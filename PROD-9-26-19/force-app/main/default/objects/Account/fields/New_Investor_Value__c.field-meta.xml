<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>New_Investor_Value__c</fullName>
    <description>New investor value for New Investor, Re-up and Crossover chart</description>
    <externalId>false</externalId>
    <formula>if(( PFG_Number_of_Original_Commitments__c = 1) &amp;&amp; (  PFG_Number_of_Current_Commitments__c	= 1), PFG_Total_Raised_Capital__c ,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>New Investor Value (If new investor, total raised capital)</inlineHelpText>
    <label>New Investor Value</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
