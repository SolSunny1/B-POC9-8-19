<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Subscription_Quarter_New__c</fullName>
    <description>Converting Subscripition date into quarter automatically</description>
    <externalId>false</externalId>
    <formula>IF(ISBLANK(Subscription_Date__c), &quot;&quot;, TEXT(YEAR(Subscription_Date__c)) + &quot; Q&quot; + TEXT(CEILING( MONTH (Subscription_Date__c) / 3 )))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Subscription Quarter New</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
