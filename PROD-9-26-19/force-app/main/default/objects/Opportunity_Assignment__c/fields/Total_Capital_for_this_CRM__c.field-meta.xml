<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_Capital_for_this_CRM__c</fullName>
    <description>This field will calculate the percentage of the invested capital (read only)</description>
    <externalId>false</externalId>
    <formula>ROUND((Total_Invested_Capital_Opport__c  * Percentage_of_Split__c),0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Total % Capital for this CRM</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
