<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validation_on_Account</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISPICKVAL(Room_Type__c ,&quot;Prospect Specific&quot;),ISBLANK (Account__c ))</errorConditionFormula>
    <errorDisplayField>Account__c</errorDisplayField>
    <errorMessage>You must choose a specific Investor to create an Prospect Specific room.</errorMessage>
</ValidationRule>
